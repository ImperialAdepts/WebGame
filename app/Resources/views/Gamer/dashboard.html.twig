{% extends 'base.html.twig' %}

{% block body %}
	<div id="wrapper">
		<h1>Souls</h1>
		<ul>
			{% for soul in souls %}
				<li>
					{{ soul.name }}
					{% if soul.incarnation is null %}connect human{% endif %}
					{% if soul.incarnation is not null %}play{% endif %}
				</li>
			{% endfor %}
		</ul>
	</div>
{% endblock %}